.preloader{position:fixed;top:0;bottom:0;left:0;right:0;z-index:99;background-color:#eef5ff;display:block}.wave{width:10rem;height:5rem;position:absolute;top:50%;left:50%;transform:translate(-50%,-50%)}.wave div{display:inline-block;width:.75rem;height:.75rem;border-radius:50%;background:#6666f6;animation:wave 2s cubic-bezier(.626,0,0,1)infinite;margin-right:.75rem}.wave div:nth-child(1){animation-delay:-.35s}.wave div:nth-child(2){animation-delay:-.26s}.wave div:nth-child(3){animation-delay:-.17s}.wave div:nth-child(4){animation-delay:-.09s}@keyframes wave{0%{transform:scale(1.5)translateY(0)}50%{transform:scale(1)translateY(3rem)}100%{transform:translateY(0)}}.map{height:50%;position:relative;z-index:10}