@import "https://fonts.googleapis.com/css?family=Open+Sans:wght@100..900|Montserrat:wght@100..900";body{font-family:open sans,sans-serif;font-weight:400;font-size:15px;-webkit-font-smoothing:antialiased;background-color:#d3d3d3;overflow-x:hidden}::selection{background:gold;color:#222}ol,ul{list-style-type:none;margin:0}img{vertical-align:middle;border:0}a{color:navy;font-family:montserrat,sans-serif;font-weight:400;font-size:16px;text-decoration:none;transition:.2s ease}a:hover{color:gold}p,.paragraph{color:#222;font-family:montserrat,sans-serif;font-weight:400;font-size:16px;line-height:1.5rem}h1,h2,h3,h4,h5,h6{color:#222;font-family:open sans,sans-serif;font-weight:700}h1,.h1{font-size:60px}h2,.h2{font-size:36px}h3,.h3{font-size:30px}h4,.h4{font-size:20px}h5,.h5{font-size:18px}h6,.h6{font-size:14px}footer{margin-top:.5rem;background-color:#708090}footer p{color:#222}.btn{font-family:open sans,sans-serif;font-weight:600;padding:auto;border-radius:10px;border-width:0;position:relative;z-index:1;transition:.2s ease}.btn-primary{background:navy;color:#d3d3d3}.btn-primary:hover{background:gold;color:#222}.preloader{position:fixed;top:0;bottom:0;left:0;right:0;z-index:99;background-color:#d3d3d3;display:block}.wave{width:10rem;height:5rem;position:absolute;top:50%;left:50%;transform:translate(-50%,-50%)}.wave div{display:inline-block;width:.75rem;height:.75rem;border-radius:50%;background:navy;animation:wave 2s cubic-bezier(.626,0,0,1)infinite}.wave div:nth-child(1){animation-delay:-.35s}.wave div:nth-child(2){animation-delay:-.26s}.wave div:nth-child(3){animation-delay:-.17s}.wave div:nth-child(4){animation-delay:-.09s}@keyframes wave{0%{transform:scale(1.5)translateY(0)}50%{transform:scale(1)translateY(3rem)}100%{transform:translateY(0)}}.map{border-radius:1rem;width:auto;height:600px;position:relative;z-index:10}.bg-primary{background-color:#d3d3d3}.bg-secondary{background-color:#708090}.navigation{background:#708090;padding:0;margin:0}.navigation .navbar{padding:0;position:relative}.navigation .navbar-collapse{position:relative}.navigation .navbar-collapse .nav-item{align-self:center}.navigation .navbar-collapse .nav-item .nav-link{padding:1rem;font-family:montserrat,sans-serif;font-weight:600;font-size:16px;color:#222;transition:.2s ease}.navigation .navbar-collapse .nav-item .nav-link:hover{color:gold}.navigation .navbar-collapse .nav-item a img{height:2em}.navigation .navbar-brand{text-align:center}.navigation .navbar-brand img{padding:1rem 0}.page-title{background-position:50%;background-repeat:no-repeat;background-size:cover;padding:5rem 0;margin-top:.5rem;border-radius:1rem}.page-title h2{color:#d3d3d3}.page-404{margin-top:12.5rem;margin-bottom:12.5rem}.page-404 h1{font-size:10rem;color:navy}